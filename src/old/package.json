{
  "name": "chatty",
  "version": "0.0.1",
  "description": "A web based alternate client for the shacknews chatty (http://www.shacknews.com/chatty).",
  "author": "Andy Christianson",
  "license": "MIT",
  "repository": "https://github.com/NiXXeD/chatty",
  "scripts": {
    "clean": "gulp clean",
    "lint": "gulp lint",
    "build": "npm install && gulp clean && gulp build",
    "start": "npm run build && http-server build -p 3000 -a localhost -o"
  },
  "dependencies": {
    "angular": "1.4.8",
    "angular-local-storage": "0.2.2",
    "angular-recursion": "1.0.5",
    "angular-route": "1.4.8",
    "angular-sanitize": "1.4.8",
    "babel": "6.1.18",
    "babel-eslint": "4.1.5",
    "babel-preset-es2015": "6.1.18",
    "babelify": "7.2.0",
    "browser-sync": "2.10.0",
    "browserify": "12.0.1",
    "browserify-shim": "3.8.11",
    "cssnano": "3.3.2",
    "del": "2.1.0",
    "eslint": "1.10.1",
    "gulp": "3.9.0",
    "gulp-angular-templatecache": "1.8.0",
    "gulp-concat": "2.6.0",
    "gulp-eslint": "1.1.0",
    "gulp-if": "2.0.0",
    "gulp-ng-annotate": "1.1.0",
    "gulp-postcss": "6.0.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.1",
    "gulp-util": "3.0.7",
    "http-server": "0.8.5",
    "lodash": "3.10.1",
    "merge2": "0.3.6",
    "postcss": "5.0.12",
    "postcss-discard-comments": "2.0.2",
    "postcss-reporter": "1.3.0",
    "precss": "1.3.0",
    "require-directory": "2.1.1",
    "stylelint": "2.3.2",
    "stylelint-config-suitcss": "1.0.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.6.1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular",
    "lodash": "global:_"
  }
}
